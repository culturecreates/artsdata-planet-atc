artist:7725 a schema:PerformingGroup;
  schema:name "Doktor Kaboom (David Epley)";
  schema:description """<p><b>David Epley</b>, popularly known as <b>Doktor Kaboom</b>, is a German/American science communicator, stage actor, and comedian. His interactive stage shows have been featured in venues like <a href=\"https://en.wikipedia.org/wiki/John_F._Kennedy_Center_for_the_Performing_Arts\" title=\"John F. Kennedy Center for the Performing Arts\">The Kennedy Center</a> and <a href=\"https://en.wikipedia.org/wiki/Orpheum_Theatre_(Memphis)\" title=\"Orpheum Theatre (Memphis)\">The Orpheum Theater</a>, and Epley has appeared on television programs like “Fox News”, “New Day Northwest,” and a number of local television affiliates. His home experiment ideas have also been featured in magazines like <a href=\"https://en.wikipedia.org/wiki/Parents_(magazine)\" title=\"Parents (magazine)\">Parents</a>. Epley is a U.S. Army veteran and was a volunteer firefighter and E.M.T. for 5 years.</p>
""";
  schema:location _:98b7f6f0e20348c9b1315f29782ab3b72223;
